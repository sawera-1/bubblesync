{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "strict": false,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "App.js"
  ],
  "exclude": [
    "node_modules",
    "Pods"
  ]
}
